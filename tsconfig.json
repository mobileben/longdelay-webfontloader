{
  "compilerOptions": {
    "outDir": "./www",
    "sourceMap": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "module": "commonjs",
    "target": "es6",
    "allowJs": true,
    "strict": true
  },
  "exclude": [
    "node_modules",
    "plugins"
  ]
}
